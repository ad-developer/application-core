<targets>
  <target name="logfile" xsi:type="File" fileName="log.txt">
    <layout>
      ${longdate} |
      TrackingId=${scopeproperty:TrackingId} |
      UserLanId=${scopeproperty:UserLanId} |
      UserFullName=${scopeproperty:UserFullName} |
      ${level} | ${message}
    </layout>
  </target>
</targets>